		script.Parent.Parent.vv.Text = "Anti-Tool"
		script.Parent.Parent.Parent.close.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.Parent.open.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.Parent.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.Parent.open.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.Parent.close.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.t1.Text = "When enabled, do not"
		script.Parent.Parent.t2.Text = "equip personal tools."
		script.Parent.Visible = false
		script.Parent.Parent["one"].Visible = true
		script.Parent.Parent.d2.Visible = false
		script.Parent.Parent.e2.Visible = false
		script.Parent.Parent.d1.Visible = true
		script.Parent.Parent.e1.Visible = true
		script.Parent.Parent.hotkeybutton.Visible = true
		script.Parent.Parent.hotkey.Visible = true
		script.Parent.Parent.hotkey2.Visible = false
		script.Parent.Parent.hotkeybutton2.Visible = false
	end
	script.Parent.MouseButton1Click:Connect(var)
end
