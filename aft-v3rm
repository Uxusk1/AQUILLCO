local function UCODYCB_fake_script() -- hotkeybutton2.LocalScript 
	local script = Instance.new('LocalScript', hotkeybutton2)

	local UserInputService = game:GetService("UserInputService")
		local SoundS = game:GetService("SoundService")
		local player = game:GetService("Players").LocalPlayer
		local char = player.Character or player.CharacterAdded:Wait()
		local pressed = false
		
		UserInputService.InputBegan:Connect(function(inputObject, gpe)
			if script.Parent.Text == "" then return end
		    local key = Enum.KeyCode[script.Parent.Text]
		    if inputObject.KeyCode == key then
		        if not pressed then
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = true
					pressed = true
		            else
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Anchored = false
		            pressed = false
		            end
					end
		       	end)
		
		UserInputService.InputEnded:Connect(function(input)
			if script.Parent:IsFocused() and input.KeyCode ~= Enum.KeyCode.Unknown then
			local msg = script.Parent
			script.Parent.Text = input.KeyCode.Name
		    if msg.Text == "" then return end
		    local key = Enum.KeyCode[msg.Text]
			end
		end)
		
end
coroutine.wrap(UCODYCB_fake_script)()
local function ZQSEU_fake_script() -- hotkeybutton.LocalScript 
	local script = Instance.new('LocalScript', hotkeybutton)

	local UserInputService = game:GetService("UserInputService")
		local SoundS = game:GetService("SoundService")
		local player = game:GetService("Players").LocalPlayer
		local char = player.Character or player.CharacterAdded:Wait()
		local pressed = false
		
		UserInputService.InputBegan:Connect(function(inputObject, gpe)
			if script.Parent.Text == "" then return end
		    local key = Enum.KeyCode[script.Parent.Text]
		    if inputObject.KeyCode == key then
		        if not pressed then
		            _G.fc = true
					spawn(function()
					    while true do
					        wait()
					        if _G.fc then
									for i,v in pairs(game:GetService("Players")["LocalPlayer"].Character:GetDescendants()) do
									if v.Name == "RightGrip" and v:IsA("Weld") then
									v.Parent = game:GetService("SoundService")
									end
					end
					end
	        end
	    end)
		            pressed = true
		        else
			_G.fc = false
		            for i,v in pairs(SoundS:GetChildren()) do
		                v.Parent = char
		            end
		            pressed = false
		        end
		    end
		end)
		
		UserInputService.InputEnded:Connect(function(input)
			if script.Parent:IsFocused() and input.KeyCode ~= Enum.KeyCode.Unknown then
			local msg = script.Parent
			script.Parent.Text = input.KeyCode.Name
		    if msg.Text == "" then return end
		    local key = Enum.KeyCode[msg.Text]
			end
		end)
end
coroutine.wrap(ZQSEU_fake_script)()
local function QHHM_fake_script() -- close.cs 
	local script = Instance.new('LocalScript', close)

	function v()
		script.Parent.Parent.BackgroundTransparency = 0
		script.Parent.Parent.main.Visible = false
		script.Parent.Parent.vv1.Visible = true
		if script.Parent.Parent.main.t1.Text == "When enabled, do not" then
			script.Parent.Parent.vv1.TextColor3 = Color3.fromRGB(78, 43, 255) else
			script.Parent.Parent.vv1.TextColor3 = Color3.fromRGB(252, 48, 48)
		end
	end
	script.Parent.MouseButton1Click:Connect(v)
end
coroutine.wrap(QHHM_fake_script)()
local function ODBVXN_fake_script() -- open.os 
	local script = Instance.new('LocalScript', open)

	function var()
		script.Parent.Parent.BackgroundTransparency = 1
		script.Parent.Parent.main.Visible = true
		script.Parent.Parent.vv1.Visible = false
		if script.Parent.Parent.main.t1.Text == "When enabled, do not" then
		script.Parent.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.e1.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.d1.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.t1.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.t2.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.tt.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.main.vv.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.close.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.open.TextColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.Parent.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.open.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.close.BorderColor3 = Color3.fromRGB(78, 43, 255)
		script.Parent.Parent.BorderColor3 = Color3.fromRGB(78, 43, 255)
		else
		script.Parent.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.e1.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.d1.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.t1.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.t2.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.tt.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.main.vv.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.close.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.open.TextColor3 = Color3.fromRGB(252, 48, 48)
		script.Parent.Parent.Parent.BorderColor3 = Color3.fromRGB(252, 48, 48)
